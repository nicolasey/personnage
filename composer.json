{
    "name": "nicolasey/personnage",
    "description": "Personnage package for RolePlay Games",
    "type": "library",
    "authors": [
        {
            "name": "nicolasey",
            "email": "nicolas.sey@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Nicolasey\\Personnage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nicolasey\\Personnage\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1.3",
        "illuminate/database": "~5.6|~5.7",
        "illuminate/events": "~5.6|~5.7",
        "illuminate/routing": "~5.6|~5.7",
        "illuminate/support": "~5.6|~5.7",
        "spatie/laravel-sluggable": "^2.1",
        "spatie/laravel-medialibrary": "^7.0.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "^3.6|^3.7",
        "phpunit/phpunit": "^7.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nicolasey\\Personnage\\PersonnageServiceProvider",
                "Nicolasey\\Personnage\\PersonnageEventServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
